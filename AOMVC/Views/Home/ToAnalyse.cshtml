@model ICollection<LibAOModels.Test>

<table class="table">
    <tr>
        <th>Kind</th>
        <th>Module</th>
        <th>Afgewerkt</th>
        <th></th>
    </tr>
    @foreach (var test in Model)
    {
        <tr>
            <td>@test.User.Firstname @test.User.Surname</td>
            <td>@test.Routine.Name</td>
            <td>@Html.DisplayFor(modelItem => test.Finisheddate.Value, "LocalDatetime")</td>
            <td>@Html.ActionLink("Analyseren", "Analyse", "Test", new { id = test.ID }, new  { @title="Analyseren"})</td>
        </tr>
    }
</table>
<a href="~/Test/ToAnalyse" class="btn" title="Meer"><i class="icon-plus"></i> &nbsp;&nbsp;&nbsp;Meer&nbsp;</a>
